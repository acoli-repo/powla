    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY conllrdf "http://purl.org/powla/conllrdf.owl#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY conll "http://ufal.mff.cuni.cz/conll2009-st/task-description.html#" >
    <!ENTITY nif "http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#" >
]>


<rdf:RDF xmlns="http://purl.org/powla/conllrdf.owl#"
     xml:base="http://purl.org/powla/conllrdf.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:conll="http://ufal.mff.cuni.cz/conll2009-st/task-description.html#"
     xmlns:powla="http://purl.org/powla/powla.owl#"
     xmlns:nif="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:conllrdf="http://purl.org/powla/conllrdf.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <rdfs:comment xml:lang="en"
            >grounding CoNLL-RDF data structures in POWLA superclasses

Note that the intended scope of this ontology is to guide the SPARQL-driven conversion from CoNLL-RDF to POWLA. It can, however, also be used to define CoNLL-RDF data structures, which are otherwise not backed by an ontology, but only by textal descriptions of the CoNLL format.</rdfs:comment>
        <owl:imports rdf:resource="http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core"/>
        <owl:imports rdf:resource="http://purl.org/powla/powla.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#nextSentence -->

    <owl:ObjectProperty rdf:about="&nif;nextSentence">
        <rdfs:subPropertyOf rdf:resource="&powla;nextNode"/>
    </owl:ObjectProperty>
    


    <!-- http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#nextWord -->

    <owl:ObjectProperty rdf:about="&nif;nextWord">
        <rdfs:subPropertyOf rdf:resource="&powla;nextNode"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#hasParent -->

    <owl:ObjectProperty rdf:about="&powla;hasParent"/>
    


    <!-- http://purl.org/powla/powla.owl#nextNode -->

    <owl:ObjectProperty rdf:about="&powla;nextNode"/>
    


    <!-- http://ufal.mff.cuni.cz/conll2009-st/task-description.html#HEAD -->

    <owl:ObjectProperty rdf:about="&conll;HEAD">
        <rdfs:subPropertyOf rdf:resource="&powla;hasParent"/>
    </owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&conll;WORD">
        <rdfs:subPropertyOf rdf:resource="&powla;hasStringValue"/>
    </owl:DatatypeProperty>

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#Sentence -->

    <owl:Class rdf:about="&nif;Sentence">
        <rdfs:subClassOf rdf:resource="&powla;Root"/>
    </owl:Class>
    


    <!-- http://persistence.uni-leipzig.org/nlp2rdf/ontologies/nif-core#Word -->

    <owl:Class rdf:about="&nif;Word">
        <rdfs:subClassOf rdf:resource="&powla;Terminal"/>
    </owl:Class>
    


    <!-- http://purl.org/powla/powla.owl#Root -->

    <owl:Class rdf:about="&powla;Root"/>
    


    <!-- http://purl.org/powla/powla.owl#Terminal -->

    <owl:Class rdf:about="&powla;Terminal"/>
</rdf:RDF>



