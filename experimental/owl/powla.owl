    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#comment -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#comment"/>
    


    <!-- http://www.w3.org/2002/07/owl#deprecated -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#deprecated"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/powla/powla.owl#hasChild -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#hasChild">
        <owl:inverseOf rdf:resource="http://purl.org/powla/powla.owl#hasParent"/>
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Nonterminal"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:comment xml:lang="en">OPTIONAL inverse of hasParent. Usage of hasParent is recommended.

Note: In POWLA 2.0, this property will be renamed to powla:child.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#hasDocument -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#hasDocument">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Layer"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Document"/>
        <rdfs:comment xml:lang="en">Note: In POWLA 2.0, this property will be renamed to powla:document</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#hasLayer -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#hasLayer">
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Layer"/>
        <rdfs:comment xml:lang="en">Note: In POWLA 2.0, this property will be renamed to powla:layer.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#hasParent -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#hasParent">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Nonterminal"/>
        <rdfs:comment xml:lang="en">RECOMMENDED property to model phrase structures, resp., tree structures in which (the string value of) the parent corresponds to (the string values of) its dependents.

Note: This is not to be used for dependency syntax. For these, use powla:Relation, instead.

Note: In POWLA 2.0, this property will be renamed to powla:parent.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#hasSource -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#hasSource">
        <owl:inverseOf rdf:resource="http://purl.org/powla/powla.owl#isSourceOf"/>
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Relation"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:comment xml:lang="en">RECOMMENDED property to link a relation with its source node.

Note: In dependency annotation, we consider the dependent the source.

Note: In POWLA 2.0, this property will be renamed to powla:source.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#hasSubDocument -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#hasSubDocument">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Document"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Document"/>
        <rdfs:comment xml:lang="en">Note: In POWLA 2.0, this property will be renamed to powla:subset.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#hasTarget -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#hasTarget">
        <owl:inverseOf rdf:resource="http://purl.org/powla/powla.owl#isTargetOf"/>
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Relation"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:comment xml:lang="en">RECOMMENDED property to link a relation with its target node.

Note: In dependency annotation, we consider the head the target. In phrase structure syntax, we consider the parent the target.

Note: In POWLA 2.0, this property will be renamed to powla:target.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#isSourceOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#isSourceOf">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Relation"/>
        <rdfs:comment xml:lang="en">OPTIONAL inverse of hasSource. Usage of hasSource is recommended.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#isTargetOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#isTargetOf">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Relation"/>
        <rdfs:comment xml:lang="en">OPTIONAL inverse of hasTarget. Usage of hasTarget is recommended.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#next -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#next">
        <owl:equivalentProperty rdf:resource="http://purl.org/powla/powla.owl#nextNode"/>
        <owl:inverseOf rdf:resource="http://purl.org/powla/powla.owl#previous"/>
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:comment xml:lang="en">RECOMMENDED property to mark adjacency between sibling nodes in phrase structure annotation.

Note that powla:next is *always* defined relative to a particular parent element. That is, the same node can have multiple successors for different types of annotation. In order to retrieve the next node for a particular kind of annotation, say, conll:PARSE, include the parent node into the query:

SELECT {
?a ?b
} WHERE {
?a nif:next ?b.
?a powla:hasParent ?parent.
?b powla:hasParent ?parent.
?parent a conll:PARSE.
}</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#nextNode -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#nextNode">
        <owl:inverseOf rdf:resource="http://purl.org/powla/powla.owl#previousNode"/>
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#nextTerm -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#nextTerm">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/powla/powla.owl#next"/>
        <rdfs:comment xml:lang="en">RECOMMENDED property that encodes the base order of a document as a partial order over terminal nodes.

Note: In the original POWLA 1.0 model, this property did not exist (but powla:next could have been used), but we would infer it from powla:start and powla:end attributes. As many corpora do, however, provide non-numerical IDs or use timestamps, instead, we generalized powla:start to any RDF literal.

Note: When POWLA is used in conjunction with a vocabulary that identifies the basic elements of annotation, e.g., NIF or WebAnnotation, this property does not have to be explicitly encoded. Instead, a corresponding function can be defined as a subclass of powla:nextTerm.

Note that powla:nextTerm is a sub-property of powla:nextWord, but it is special in that it does not have a common parent node (this is implicitly represented by the sentence). For disentangling powla:nextTerm and powla:next, check whether two nodes have the same parent node (see powla:next).

Corresponds to nif:nextWord in CoNLL-RDF, the (implicit) order of tokens in PAULA 1.0 and of terminals in PAULA 1.1.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#previous -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#previous">
        <owl:equivalentProperty rdf:resource="http://purl.org/powla/powla.owl#previousNode"/>
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:comment xml:lang="en">OPTIONAL property to mark adjacency between sibling nodes in phrase structure annotation. Recommended property is powla:next.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/powla/powla.owl#previousNode -->

    <owl:ObjectProperty rdf:about="http://purl.org/powla/powla.owl#previousNode">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/powla/powla.owl#documentID -->

    <owl:DatatypeProperty rdf:about="http://purl.org/powla/powla.owl#documentID">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Document"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/powla/powla.owl#end -->

    <owl:DatatypeProperty rdf:about="http://purl.org/powla/powla.owl#end">
        <owl:equivalentProperty rdf:resource="http://purl.org/powla/powla.owl#endPosition"/>
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">OPTIONAL property to mark the end of a terminal (or, in the query profile, any node). Should correspond to the start of the next terminal.
Note: This is a convenience function for querying. In the core profile, this property should be inferred from the start of the next terminal.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/powla/powla.owl#endPosition -->

    <owl:DatatypeProperty rdf:about="http://purl.org/powla/powla.owl#endPosition">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/powla/powla.owl#hasAnnotation -->

    <owl:DatatypeProperty rdf:about="http://purl.org/powla/powla.owl#hasAnnotation">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#POWLAElement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">RECOMMENDED ABSTRACT property to express values of linguistic annotation (over nodes and relations) or metadata (over layers and datasets).

Note: This property should not be used directly. Instead, defined subproperties which represent a particular type of linguistic annotations. If general metadata about annotation layers is to be expressed, it is recommended to not reify hasAnnotation, but instead attach the metadata to the node or the layer. 

Note: For subproperties of hasAnnotation, we recommend the following naming pattern: The attribute name is specified in the hasXY property, the value is the string value.

Corresponds to labels attached to nodes and edges in SALT</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/powla/powla.owl#hasStringValue -->

    <owl:DatatypeProperty rdf:about="http://purl.org/powla/powla.owl#hasStringValue">
        <owl:equivalentProperty rdf:resource="http://purl.org/powla/powla.owl#string"/>
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/powla/powla.owl#layerID -->

    <owl:DatatypeProperty rdf:about="http://purl.org/powla/powla.owl#layerID">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Layer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/powla/powla.owl#start -->

    <owl:DatatypeProperty rdf:about="http://purl.org/powla/powla.owl#start">
        <owl:equivalentProperty rdf:resource="http://purl.org/powla/powla.owl#startPosition"/>
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">RECOMMENDED property to mark the position of a Terminal with a timestamp symbol. Usage for other types of nodes is optional.
Note that we do not require these symbols to be numbers.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/powla/powla.owl#startPosition -->

    <owl:DatatypeProperty rdf:about="http://purl.org/powla/powla.owl#startPosition">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/powla/powla.owl#string -->

    <owl:DatatypeProperty rdf:about="http://purl.org/powla/powla.owl#string">
        <rdfs:domain rdf:resource="http://purl.org/powla/powla.owl#Node"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">RECOMMENDED property to assign a Terminal a surface string. Usage for other node types is optional.</rdfs:comment>
    </owl:DatatypeProperty>
    


